    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "lucide-react": "^0.462.0",
    "next-themes": "^0.3.0",
    "react": "^18.3.1",
    "react-day-picker": "^8.10.1",
