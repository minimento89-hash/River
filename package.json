{
  "name": "my-mind-studio",
  "private": true,
  "version": "1.0.0",
  "description": "My Mind desktop application built with Electron and React.",
  "main": "main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "lint": "eslint .",
    "preview": "vite preview",
    "electron-start": "electron .",
    "electron-dev": "concurrently -k \"npm run dev -- --port 8080\" \"wait-on http://localhost:8080 && cross-env NODE_ENV=development VITE_DEV_SERVER_URL=http://localhost:8080 npm run electron-start\"",
    "electron-build": "npm run build && electron-builder",
    "start": "electron .",
    "postinstall": "electron-builder install-app-deps",
    "test": "jest"
  },
